{
    "name": "gapi",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "description": "gapi-front",
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "prepare": "husky",
        "dev": "vite",
        "build": "tsc && vite build --mode prod",
        "test": "vitest",
        "test:staged": "vitest --run",
        "lint": "eslint .",
        "lint:lint-staged": "lint-staged",
        "preview": "vite preview"
    },
    "lint-staged": {
        "*.{jsx,ts,tsx}": [
            "prettier --write",
            "eslint --fix"
        ],
        "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
            "prettier --write--parser json"
        ],
        "*.json": [
            "prettier --write"
        ],
        "*.{scss,less,styl,html}": [
            "prettier --write"
        ],
        "*.md": [
            "prettier --write"
        ]
    },
    "dependencies": {
        "@gsap/react": "^2.1.2",
        "@radix-ui/react-slot": "^1.2.4",
        "await-to-js": "^3.0.0",
        "axios": "^1.13.4",
        "class-variance-authority": "^0.7.1",
        "classnames": "^2.5.1",
        "clsx": "^2.1.1",
        "dayjs": "^1.11.19",
        "gsap": "^3.14.2",
        "immer": "^11.1.3",
        "lodash-es": "^4.17.23",
        "lucide-react": "^0.563.0",
        "md5": "^2.3.0",
        "nanoid": "^5.1.6",
        "nuqs": "^2.8.8",
        "p-limit": "^7.3.0",
        "react": "^19.2.4",
        "react-dom": "^19.2.4",
        "react-router": "^7.13.0",
        "react-use": "^17.6.0",
        "tailwind-merge": "^3.4.0",
        "vite-plugin-svgr": "^4.5.0",
        "zod": "^4.3.6",
        "zustand": "^5.0.11"
    },
    "devDependencies": {
        "@commitlint/cli": "^20.4.1",
        "@commitlint/config-conventional": "^20.4.1",
        "@eslint/js": "^10.0.1",
        "@nabla/vite-plugin-eslint": "^2.0.6",
        "@tailwindcss/vite": "^4.1.18",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.2",
        "@testing-library/user-event": "^14.6.1",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^25.2.1",
        "@types/react": "^19.2.13",
        "@types/react-dom": "^19.2.3",
        "@vitejs/plugin-react": "^5.1.3",
        "baseline-browser-mapping": "^2.9.19",
        "eslint": "^9.39.1",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-refresh": "^0.5.0",
        "globals": "^17.3.0",
        "husky": "^9.1.7",
        "jsdom": "^28.0.0",
        "lint-staged": "^16.2.7",
        "prettier": "^3.8.1",
        "tailwindcss": "^4.1.18",
        "tw-animate-css": "^1.4.0",
        "typescript": "~5.9.3",
        "typescript-eslint": "^8.54.0",
        "vite": "^7.3.1",
        "vitest": "^4.0.18"
    },
    "engines": {
        "node": "22.16.0",
        "pnpm": "10.24.0"
    },
    "volta": {
        "node": "22.16.0",
        "pnpm": "10.24.0"
    },
    "keywords": [
        "React",
        "TypeScript",
        "Vite"
    ],
    "author": "zhangzhouou@gmail.com"
}
